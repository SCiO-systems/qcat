msgctxt "cca helptext"
msgid ""
"<strong>Example:</strong> stone lines: 250 m, dam: 20’000 m<sup>3</sup>"
msgstr "<strong>Ejemplo:</strong> hileras de piedra: 250 m, dique: 20’000 m<sup>3</sup>"

